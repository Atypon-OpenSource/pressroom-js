            </mml:msub>
            </mml:mrow>
          </mml:mfenced><mml:mo>+</mml:mo><mml:msub>
            <mml:mi>φ</mml:mi>
            <mml:mi>i</mml:mi>
          </mml:msub>
            <mml:mo>+</mml:mo><mml:msub>
            <mml:mi>δ</mml:mi>
            <mml:mi>t</mml:mi>
          </mml:msub>
            <mml:mo>+</mml:mo><mml:msub>
