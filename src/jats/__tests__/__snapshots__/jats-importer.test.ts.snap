                    </mml:msub>
                    </mml:mrow>
                  </mml:mfenced><mml:mo>/</mml:mo><mml:mi>m</mml:mi><mml:mi>i</mml:mi><mml:mi>d</mml:mi><mml:mi>p</mml:mi><mml:mi>o</mml:mi><mml:mi>i</mml:mi><mml:mi>n</mml:mi><mml:msub>
                  <mml:mi>t</mml:mi>
                  <mml:mi>k</mml:mi>
                </mml:msub>
