            <mml:mo stretchy=\\"false\\">)</mml:mo><mml:mo>+</mml:mo><mml:mi>τ</mml:mi><mml:mfenced>
            <mml:mrow>
              <mml:mi>T</mml:mi><mml:mi>r</mml:mi><mml:mi>u</mml:mi><mml:mi>m</mml:mi><mml:msub>
              <mml:mi>p</mml:mi>
